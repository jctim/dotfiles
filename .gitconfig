[user]
	name = Illia Tkachuk
	email = tkachuk.i.m@gmail.com
[core]
	excludesfile = ~/.gitignore_global
	autocrlf = input
[color]
	ui = true
	diff = true
[filter "media"]
	required = true
	clean = git media clean %f
	smudge = git media smudge %f
[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  dc = diff --cached
  lg = log -p
  who = shortlog -s --
  fuckme = reset --hard HEAD
  fuckyou = push --force
[commit]
	template = ~/.stCommitMsg
